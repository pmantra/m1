            },
            "meta": {
                "icon": "https://storage.googleapis.com/maven-prod-svg/assessments/postpartum/5.svg",
                "subtext": "* We don't share this with anyone"
            }
        },
        {
            "id": 6,
            "body": "What's your favorite hobby?",
            "widget": {
                "type": "freetextfield",
