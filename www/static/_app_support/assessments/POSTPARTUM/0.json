        {
            "id": 2,
            "body": "How are you feeling?",
            "widget": {
                "type": "range",
                "max": 3,
                "options": [
                    {
                        "value": 0,
                        "label": "0 - not good"
                    },
                    {
                        "value": 1,
                        "label": "1 - could be better"
                    },
                    {
                        "value": 2,
                        "label": "2 - okay"
                    },
                    {
                        "value": 3,
                        "label": "3 - good"
                    }
                ]
            },
            "meta": {
                "icon": "https://storage.googleapis.com/maven-prod-svg/assessments/postpartum/2.svg",
                "subtext": "* Feelings can be approximate"
