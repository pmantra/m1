msgstr "Wellness Coach"

msgid "vertical_wellness_coach_pluralized_display_name"
msgstr "Wellness Coaches"

msgid "vertical_wellness_coach_description"
msgstr "Healthy living, emotional support, wellness, nutrition"

msgid "vertical_wellness_coach_long_description"
