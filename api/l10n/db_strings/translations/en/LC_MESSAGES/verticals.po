msgid "vertical_highrisk_pregnancy_coach_name"
msgstr "High-risk Pregnancy Coach"

msgid "vertical_highrisk_pregnancy_coach_display_name"
msgstr "High-Risk Pregnancy Coach"

msgid "vertical_highrisk_pregnancy_coach_pluralized_display_name"
msgstr "High-Risk Pregnancy Coaches"

